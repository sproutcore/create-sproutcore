<% include('../copyright_block_js.ejs') %>
/*globals <%= namespace %> */

<%= namespace %>.Item = SC.Record.extend({
    primaryKey: 'id',
    title: SC.Record.attr(String),
    isDone: SC.Record.attr(Boolean, { defaultValue: false }),
    createdAt: SC.Record.attr(Number, { defaultValue: function() { return new Date().getTime(); } })
});
